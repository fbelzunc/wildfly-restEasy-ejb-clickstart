{
    "id": "wildfly-restEasy-ejb-clickstart",
    "name": "Wildfly8 RestEasy App",
    "source": "github",
    "description" : "Basic RestEasy Java EE 7 app deployed on Wildfly 8 Web Profile via Jenkins with maven",
    "doc-url" : "https://github.com/CloudBees-community/wildfly-restEasy-ejb-clickstart/blob/master/README.md",
    "icon" : "https://raw.github.com/CloudBees-community/wildfly-restEasy-ejb-clickstart/master/icon.png",
    "disabled" : false,
    "supported-by" : "community",
   // "app-package" : "https://community.ci.cloudbees.com/job/wildfly-restEasy-ejb-clickstart/lastSuccessfulBuild/artifact/target/wildfly8-clickstart-1.0.0-SNAPSHOT.war",

    "runtime": {
        "type": "wildfly8",
        "app-parameters" : {
            "runtime.jboss.auth-realm.database": "mydb"
        }
    },
    "databases": [
        {
            "type": "mysql",
            "alias": "mydb"
        }
    ],
    "build-with-jenkins": {
        "template": {
            "type": "maven",
            "options": {
            }
        }
    }
}